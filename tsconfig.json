{
  "compilerOptions": {
    "isolatedModules": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "module": "esnext",
    "noImplicitAny": false,
    "removeComments": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "preserve",
    "noLib": false,
    "target": "es5",
    "lib": ["ESNext", "DOM"],
    "paths": {
      "@/*": ["table-sticky-plugin/*"]
    },
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationDir": "./temp"
  },
  "exclude": ["src", "table-sticky-plugin"],
  "include": [],
  "files": [
    "table-sticky-plugin/index.ts",
    "table-sticky-plugin/table-sticky.ts",
    "table-sticky-plugin/type.ts",
    "table-sticky-plugin/utils.ts"
  ]
}
